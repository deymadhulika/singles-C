#include <stdio.h>

int main()
{
   int i, term, num1 = 0, num2 = 1 ;
   
   int next_term = num1+num2 ;
   
   printf("Enter the number of terms : ");
   scanf("%d", &term);
   printf("The fibonacci series is %d\t%d\t", num1,num2);
   
    for (i=3; i<=term; i++)
    {
        printf("%d\t",next_term);
        num1 = num2 ;
        num2 = next_term ;
        next_term = num1+num2 ;
      
    }
  
    return 0;
}
