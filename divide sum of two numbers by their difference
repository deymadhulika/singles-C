#include<stdio.h>
int main()
{
    int n1 , n2 ;
    float result ;
    
    printf("Enter two numbers: ");
    scanf("%d %d", &n1,&n2);
    
    if (n1 == n2)
    printf("Result can't be produced as the denominator is 0");
    else {
    result = (n1+n2)/(n1-n2);
    printf("The result is : %f", result);}
    
    return 0 ;
}
